@model List<User>

@{
    ViewBag.Title = "Users";
}
@Html.Partial("_MaintenancePartial")
<head>
    <link rel="stylesheet" type="text/css" href="~/Content/Site.css">
</head>
<aside style="margin-top:10px">
    <div class="panel" @*style="border:1px solid"*@>
        <div style="margin-left: 20px">
            <h4>Maintenance > List of Users</h4>
            <button style="max-width:90px" class="btn btn-success" onclick="location.href='@Url.Action("AddUser", "Maintenance")'">Add</button>
        </div>
        <table class="table">
            <tr>
                <th>ID</th>
                <th>User Name</th>
                <th>Action</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td width="10%">@item.UserId</td>
                    <td width="50%">@item.UserName</td>
                    <td width="20%">@Html.ActionLink("Edit", "Edit", "Maintenance", new { userId = item.UserId.ToString() }, null)</td>
                    @*<td width="5%"><input class="btn-edit" type="button" value="Edit" onclick="location.href='@Url.Action("Edit", "Home")'" /></td>*@
                </tr>
            }
        </table>
    </div>
</aside>
