@model List<UnitOfMeasure>

@{
    ViewBag.Title = "UnitOfMeasures";
}
@Html.Partial("_MaintenancePartial")
    <aside style="margin-top:10px">
        <div class="panel" @*style="border:1px solid"*@>
            <div style="margin-left: 20px">
                <h4>Maintenance > Unit of Measure</h4>
                <button style="max-width:90px" class="btn btn-success" onclick="location.href='@Url.Action("ExportUnitOfMeasures", "Maintenance")'">Export</button>
            </div>
            <table class="table">
                <tr style="font-size:16px">
                    <th>ID</th>
                    <th>Unit of Measure</th>
                    <th>Abreviation</th>
                </tr>
                @foreach (var item in Model)
            {
                    <tr style="font-size:16px">
                        <td>@item.UnitOfMeasureId</td>
                        <td>@item.UnitOfMeasure1</td>
                        <td>@item.UnitOfMeasureAbbreviation</td>
                    </tr>
                }
            </table>
        </div>
</aside>
<footer>
    <p style="text-align:center; font-size:16px">Total Pages:1 Records:@Model.Count</p>
</footer>